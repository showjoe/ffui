{
  "name": "@formfactory/ffui",
  "license": "MIT",
  "version": "0.1.43",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name ffui ./src/components/index.js",
    "build-async": "vue-cli-service build --target wc-async --inline-vue --name ffui ./src/components/*.vue",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs"
  },
  "main": "./dist/ffui.common.js",
  "module": "./dist/ffui.umd.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "@popperjs/core": "^2.0.6",
    "axios": "^0.18.0",
    "bootstrap": "^4.5",
    "gsap": "^2.1.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "vue": "^2.6.6",
    "vue-cli-plugin-moment": "^0.1.1",
    "vue-cli-plugin-webpack-bundle-analyzer": "^2.0.0",
    "vue-dummy": "^1.1.3",
    "vue-inputmask": "^0.2.1",
    "vue-router": "^3.0.3",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1",
    "@fortawesome/free-solid-svg-icons": "^5",
    "@fortawesome/vue-fontawesome": "^0.1",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.9.0",
    "babel-eslint": "^10.0.1",
    "d3": "^5.9.7",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "1.3.0",
    "vuepress-plugin-demo-code": "^0.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "$": false,
      "jQuery": false,
      "moment": false,
      "module": false,
      "logger": false
    },
    "rules": {
      "no-console": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "resolutions": {
    "watchpack": "1.6.1"
  }
}
